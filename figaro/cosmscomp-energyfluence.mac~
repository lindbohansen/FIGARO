# Mesh 902cm
/score/create/boxMesh myMesh902cm
/score/mesh/boxSize 250. 147. 0.05 cm
/score/mesh/nBin 500 294 1
# -958.5 + 902
/score/mesh/translate/xyz -151.5 0 -56.5 cm 
#/score/quantity/cellFlux gammaFlux
#/score/filter/particle gammaFilter gamma
/score/quantity/cellFlux gammaFlux_1.0MeV_1.5MeV
/score/filter/particleWithKineticEnergy gamma_1.0MeV_1.5MeV 1.0 1.5 MeV gamma
/score/quantity/cellFlux gammaFlux_0.6MeV_1.0MeV
/score/filter/particleWithKineticEnergy gamma_0.6MeV_1.0MeV 0.6 1.0 MeV gamma
/score/quantity/cellFlux gammaFlux_0.4MeV_0.6MeV
/score/filter/particleWithKineticEnergy gamma_0.4MeV_0.6MeV 0.4 0.6 MeV gamma
/score/quantity/cellFlux gammaFlux_0.2MeV_0.4MeV
/score/filter/particleWithKineticEnergy gamma_0.2MeV_0.4MeV 0.2 0.4 MeV gamma
/score/close

